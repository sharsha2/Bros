<h3>Task</h3>

<a href="/tasks/add"> Add a New task</a>
<table class="table table-striped table-hover">
    <tr>
        <th>Name</th>
        <th>Priority</th>
        <th>Complete</th>
        <th>Actions</th>
    </tr>

    {{#each tasks}}
    <tr>
        <td> {{this.name}} </td>
        <td> {{this.priority}} </td>
        <td> {{this.complete}} </td>
        <td>
            <a href="/tasks/edit/{{ this._id }}"> Edit </a>|
            <a href="/tasks/delete/{{ this._id }}" class="test-danger delete"> Delete </a>
        </td>
    </tr>

    {{/each}}

</table>





















